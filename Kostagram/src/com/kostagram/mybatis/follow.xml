<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="follow">
   <insert id="insert" parameterType="com.kostagram.service.beans.FollowVO">
      insert into follow values (#{from_email}, #{to_email}, #{reg_date})
   </insert>
   
   <insert id="delete" parameterType="com.kostagram.service.beans.FollowVO">
      update follow set dispYn="N" where to_email= #{to_email}
   </insert>
</mapper>
