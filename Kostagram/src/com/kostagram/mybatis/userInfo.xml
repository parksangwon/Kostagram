<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="userInfo">
   <insert id="insert" parameterType="com.kostagram.service.beans.UserInfoVO">
      insert into person values (person_seq.nextval, #{name}, #{age}, #{job})
   </insert>
   
   <update id="update" parameterType="com.kostagram.service.beans.UserInfoVO">
      
   </update>
   
   <select id="finduser" resultType="com.kostagram.service.beans.UserInfoVO">
      select * from member where nickname='nickname' </select> 
   <select id="findList" resultType="com.kostagram.service.beans.UserInfoVO">
      
   </select> 
</mapper>
