<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="report">
   <insert id="insert" parameterType="com.kostagram.service.beans.ReportVO">
      insert into report values (#{seq_report}, #{seq_photo}, #{comment_id},SYSDATE, #{email})
   </insert>
   	    
   <select id="findList" resultType="com.kostagram.service.beans.ReportVO">
   	  select * from report a, report_Item b where a.content_id = b.content_id
   </select>                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 
</mapper>
